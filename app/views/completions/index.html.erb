<div class="row">
  <div class="medium-8 columns">
    <ul>
      <% @completions.each do |completion| %>
      <li><%= String.new << completion.course.instructional_unit << ' ' << completion.course.course_number << ' - ' << completion.grade %></li>
      <% end %>
    </ul>
  </div>
  <div class="large-4 columns">
    <h3>Actions</h3>
    <ul class="side-nav panel">
      <li><%= link_to 'Add a completed course', new_completion_path %></li>
    </ul>
  </div>
</div>